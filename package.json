{
  "name": "spec-md-confluence",
  "version": "2.0.0",
  "main": "./src/index.js",
  "author": "Michael McCord",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@semantic-release/changelog": "5.0.x",
    "@semantic-release/git": "9.0.x",
    "jest-diff": "25.3.x",
    "semantic-release": "17.0.x"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "test": "node ./test/runner.js",
    "watch": "nodemon -e css,js,json,pegjs,md --ignore src/grammar.js --exec 'yarn test'",
    "release": "semantic-release"
  },
  "files": [
    "src",
    "LICENSE",
    "templates",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "ast",
    "ecma",
    "ecmascript",
    "html",
    "markdown",
    "markup",
    "spec",
    "specs",
    "specification",
    "typography",
    "code",
    "confluence",
    "spec-md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelmccord/spec-md-confluence"
  },
  "bugs": {
    "url": "https://github.com/michaelmccord/spec-md-confluence/issues"
  },
  "dependencies": {
    "handlebars": "4.7.x",
    "handlebars-helpers": "0.10.x",
    "spec-md": "https://github.com/michaelmccord/spec-md.git#f15eff3ebf6572b16b2bb5b23ebff5f455ba094c",
    "uri-js": "^4.2.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "spec-md-confluence Changelog",
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release):  ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/*.tgz",
              "name": "npm package",
              "label": "npm package"
            }
          ]
        }
      ]
    ]
  }
}
